<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/cardiact_connect_theme</item>
        <item name="colorPrimaryDark">#880E4F</item>
        <item name="colorAccent">@color/sig_strength_mid</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="DialogBoxAnimation">
        <item name="android:windowEnterAnimation">@anim/slide_up</item>
        <item name="android:windowExitAnimation">@anim/slide_down</item>

    </style>
    <style name="DialogBoxAnimation_left_in_right_out">
        <item name="android:windowEnterAnimation">@anim/trans_right_in</item>
        <item name="android:windowExitAnimation">@anim/trans_right_out</item>

    </style>







    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
